{
  "expo": {
    "name": "to-do-hub",
    "slug": "to-do-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.todohub",
      "permissions": [
        "INTERNET"
      ],
      "config": {
        "usesCleartextTraffic": true
      }
    },
    "extra": {
      "eas": {
        "projectId": "8c5506b1-84df-4350-8753-81f230f9defb"
      }
    }
  }
}
